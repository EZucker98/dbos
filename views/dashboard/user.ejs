  <style>
      .title img.status {
    opacity: 1;
    -webkit-animation: header_NotificationCircle_ANIM_err 1.0s infinite ease-in-out;
    -moz-animation: header_NotificationCircle_ANIM_err 1.0s infinite ease-in-out;
    -o-animation: header_NotificationCircle_ANIM_err 1.0s infinite ease-in-out;
    animation: header_NotificationCircle_ANIM_err 1.0s infinite ease-in-out;
    -webkit-transition: all .25s ease-in-out;
    -moz-transition: all .25s ease-in-out;
    -o-transition: all .25s ease-in-out;
    transition: all .25s ease-in-out;
    }
  </style>
