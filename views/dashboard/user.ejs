<body>
  <nav class="navbar navbar-expand navbar-dark">
    <div class="nav navbar-nav">
      <div class="icon">
       <span style="color: var(--primary);">D</span>BOS
      </div>
    <a class="nav-item nav-link" href="/docs" title="Docs">
      <i class="fa fa-book"></i>
    </a>
    <a class="nav-item nav-link" href="/users" title="Users">
      <i class="fa fa-users"></i>
    </a>
    <a class="nav-item nav-link" href="https://discord.gg/ttRXkVUGjk" title="Support">
      <i class="fab fa-discord"></i>
    </a>
    <a class="nav-item nav-link" href="https://github.com/wezacon/dbos" title="Github">
      <i class="fab fa-github"></i>
    </a>
  </div>
  <div class="nav navbar-nav ml-auto">

    <!-- <div>
        Right Side
    </div> -->

  </div>
</nav>
  <% if(dbuser.removed == false){ %>
    <% if(dbuser.allowListing == false){ %>
      <div class="topBar info-background">
        <div class="content">
          <i class="fa fa-info-circle" style="color: var(--primary)!important; filter: brightness(40%) !important;"></i>
          &nbsp;
          This profile is <strong>not</strong> listed
      </div>
    </div>
    <% } %>  
  <% } %>  
  <% if(dbuser.removed == true){ %>
    <div class="topBar error-background">
        <div class="content">
          <i class="fa fa-exclamation-triangle"></i>
          &nbsp;   This user has been removed
          <div class="reason">
            <div class="sectionHeader" style="padding-right: 200px;padding-left: 200px;">
              <div>
                Reason&nbsp;&nbsp;
              </div>
              <div class="line"></div>
            </div>
            <strong><%- dbuser.removeReason %></strong>
          </div>
      </div>
    </div>
  <% } %> 
 <div class="header">
  <div class="inner-header">
    <div class="title">
      <br>
      <br>
      <div class="userIcon">
      <img class="icon user" style="border-radius: 50%;" src="<% if(dbuser.removed == false){ %><%= avatar %><% } else {%> <%= icon %> <% } %>">
      <% if(dbuser.removed == false){ %>    
            <% if(admin == true){ %>
                <div class="teamBadge admin"><span>ADMIN</span></div>
            <% } }%>  
      </div>

    <h1 style="font-size: 30px !important;">
      <% if(dbuser.removed == false){ %> 
        <%= username %>
      <% } else { %>
       <strong style="font-weight: 900;">Removed</strong> 
      <% } %>
     <% if(dbuser.removed == false){
        if(verified){ %>
              <img src="https://github.com/wezacon/dbos/blob/main/public/svg/verified.svg?raw=true" style="display: inline-block;width: 24px;" title="Verified">
          <% } 
      }
        %> 
         <% 
         if(dbuser.removed == false){
          if(developer){ %> 
              <img class="developerBadge" style="background: none !important;" src="https://emoji.gg/assets/emoji/4228_discord_bot_dev.png"> 
          <% }
          } %> 
         <div class="profilebadges" style="position: relative; justify-content: center!important; align-items: center; text-align: center; display: flex; margin: 30px;">
            <%
            if(dbuser.removed == false){
              if(contributor == true){ %>
                  <div title="contributor" class="smile" style="background-color: #FFD700; width: 38.5321%; margin-bottom: 10px;"></div>
              <% } 
            }%>  
         </div>
        </h1>
        <% if(dbuser.removed == false){ %>
          <h4 class="rankLev"><%= dbuser.messages %> Messages</h4>
        <% } %>
        <% if(dbuser.removed == false){ %>
          <% if(hasWork == true){ %>
            <br>
            <br>
          <h4 class="rankLev"><span style="color: var(--font-gray); filter: brightness(80%);">occupation:</span> <%= occupation %></h4>
          <% } %>
        <% } %>
        <div class="container" style="padding-bottom: 0px !important;">
            <% 
            if(dbuser.removed == false){
            if(hasServer){ %>
                <a class='btn_main dbtn' href="<%= serverInvite %> " target="_blank">
                  <i class="fab fa-discord"></i> Discord Server
                </a>
            <% } } %>  
            <% 
            if(dbuser.removed == false){
            if(hasGit){ %>
                <a class='btn_main dbtn' href="<%= gitLink %> " target="_blank">
                    <i class="fa fa-github"></i> Github
                </a>
            <% } 
              }
            %>  
        </div>
        <a class='btn_main dbtn' href="/">
                    Home
                  </a>
                <a class='btn_main dbtn' href="/users">
                    Users
                  </a>
    </div>
  </div>
  <br>
  <br>
  <br>
  <div class="pageContent">
