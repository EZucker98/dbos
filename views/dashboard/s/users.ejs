    <br>
    <div class="heading">
      <br>Most active users 
      <!-- <div style="width: 38.5321%;">
        <div class="smile" style="background-color: #FFD700;"></div>
      </div>  -->
    </div>
    <div class="content">
      <% if(GuildDB.premium == true){ %>
        <div class="cards">
          
                  <% nusers.forEach((user, i) => {
                    if(user.removed == false){  
                    const Rank = i + 1;
                  %>
            <div class="card">
                <div>
                <img src="<%= user.profileImage %>" style="position: relative;" alt="">
                </div>
                <br>
                <br>
            <h3>
                <%= user.username %>          
                <% if(user.serverSuperAdmin){ %>
                    <i class="fa fa-tools" style="color: #25a109;" title="Server Staff"></i>
                <% } %> 
                <% if(user.serverAdmin){ %>
                    <i class="fa fa-gavel" style="color: #2fb312;" title="Admin"></i>
                <% } %> 
                <% if(user.serverModerator){ %>
                    <i class="fa fa-wrench" style="color: #2fb312;" title="Moderator"></i>
                <% } %> 
            </h3>
            <h4 class="rankLev" style="position: absolute; max-width: 40px; top:-15px; left:-20px;">#<%= Rank %></h4>
                  <br>
                  <br>
            <h4 class="rankLev"><%= user.messages %> Messages</h4>
            <% if(user.siteAdmin == true){ %> <div class="admin">ADMIN</div> <% } %> 
            <div style="display: flex!important;">
              <a class='btn_main dbtn' href="u/<%= user.id %>">
                    Server Profile
                  </a>
                      <a class='btn_main dbtn' href="../user/<%= user.userID %>">
                        Global Profile
                      </a>
                  </div>
                
            </div>
            
            
      <% 
          }
        }); 
          %>

        </div>
        <% } else {%>
            <div style="text-align: center !important;">
              <h2>This server does not have premium.</h2>
            </div>
            <% } %> 

        <div class="Buttons">
            <a href="/api/git"><i class="fab fa-github-square"></i></a>
            <a href="https://dbos.flarum.cloud/"><i class="fas fa-question-circle"></i></a>
        </div>
    </div>
