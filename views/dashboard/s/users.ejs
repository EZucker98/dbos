 <div class="header">
  <div class="inner-header">
    <div class="title">
      <br>
      <br>
      <img class="icon alive" src="<%= serverCore.iconURL() %>">
      <h1><%= serverCore.name %> - Users</h1>
    </div>
      <% if(GuildDB.premium == true){ %>
        <div class="container">
        <% if(hasInv == true){ %>
          <a class='btn_main dbtn' href="https://discord.gg/<%= GuildDB.invite %>" style="font-size: 23px !important;" target="_blank">
            <i class="fab fa-discord"></i> &nbsp; Join server
          </a> 
        </div>
        <% } %>
      <% } %>
      <div class="container">
      <a class='btn_main dbtn' href="/s/<%= serverCore.id %> ">
        Server Home
      </a>
    </div>
  </div>
  <br>
  <br>
  <br>
<div class="pageContent">
