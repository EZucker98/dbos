      <br>New users 
      <!-- <div style="width: 38.5321%;">
        <div class="smile" style="background-color: #FFD700;"></div>
      </div>  -->
    </div>
    <div class="content">
        <div class="cards">
      <% users.forEach(user => { 
          if(user.removed == false){ 
        %>
            <div class="card">
                <div>
                <img src="<%= user.profileImage %>" style="position: relative;" alt="">
                <% if(user.contributor){ %> <div title="contributor" class="smile" style="background-color: #FFD700;border: 4px solid var(--bg-2)!important; width: 38.5321%; position: absolute; top: 95px; right: 60px;"></div> <% } %>                    
                </div>
                <br>
                <br>
            <h3>
                <%= user.username %>          
                <% if(user.verified){ %>
                    <img src="https://github.com/wezacon/dbos/blob/main/public/svg/verified.svg?raw=true" style="display: inline-block;width: 24px;" title="Verified">
                <% } %> 
            </h3>
            <% if(user.admin == true){ %> <div class="admin">ADMIN</div> <% } %> 
                <a class='btn_main dbtn' href="user/<%= user.id %>">
                    View
                  </a>
            </div>
            
            
      <% 
          }
        }); 
          %>
        </div>

        <div class="Buttons">
            <a href="/api/git"><i class="fa fa-github-square"></i></a>
        </div>
    </div>
