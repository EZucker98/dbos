    <div class="content" style="justify-content: center;">
        <div class="cards">
            <% Udata.forEach((user, i) => { 
            const Rank = i + 1;
            if(user.level == 0){
                var NXP = 1;
            } else if(user.level == 1){
                var NXP = 2;
            } else {
                var NXP = user.level;
            }

            const clevel = 50;

            const nxp = user.nxp;

                %>
                  <div class="card">
                      <div style="position: relative;">
                      <img src="<%= user.userImage %>" style="position: relative;" alt="">                   
                      <% if(user.siteAdmin == true){ %>
                <% if(user.id == "557218928368156674"){ %>
                  <div class="admin" style="right: 96px;">DEVELOPER</div>
                  <% } else if(user.id == "766238334930386944"){ %>
                    <div class="admin" style="right: 113px;">SYSTEM</div>
                    <% } else {%>
                      <div class="admin">ADMIN</div>
                  <% } %>
                <% } %>
                      </div>
                      <br>
                      <br>
                  <h3>
                      <%= user.userTag %>  
                      <% if(user.siteBugBuster){%>
                        <i class="fas fa-bug" title="DBOS bugBuster" style="color: #9b59b6;"></i>  
                     <%}%>   
                      <br>                
                      <% if(user.siteModerator){ %>
                        <img src="https://github.com/wezacon/dbos/blob/main/public/img/mods/moderator.png?raw=true" style="display: inline-block;width: 40px; background: none;" title="Site Moderator">
                      <% } %>  
                      <% if(user.sitePartner){%>
                          <i class="fas fa-handshake" title="DBOS Partner" style="color: var(--primary);"></i>  
                      <%}%>  
                  </h3>            
                  <h4 class="rankLev" style="position: absolute; max-width: 40px; top:-15px; left:-20px;">#<%= Rank %></h4>
                  <br>
                  <br>
                  <h4 class="rankLev">Level: <%= user.level %> &nbsp;  XP: <%= user.xp %> / <%= nxp %></h4>
                  <% if(GuildDB.premium == true) {%>
                  <div style="display: flex!important;">
                    <% } else { %>
                      <div style="display: flex!important; justify-content: center;">
                  <% } %>
                  <% if(GuildDB.premium == true){ %>
                      <a class='btn_main dbtn' href="../s/<%= GuildDB.id %>/u/<%= user.userID %>">
                        Server Profile
                      </a>
                      <a class='btn_main dbtn' href="../user/<%= user.userID %>">
                        Global Profile
                      </a>
                  <% } else { %>
                    <a class='btn_main dbtn' href="../user/<%= user.userID %>">
                        Global Profile
                    </a>
                  <% } %>

                  </div>

                  </div>
                  
                  
            <% }); %>
