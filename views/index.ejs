                <path id="gentle-wave" d="M-160 44c30 0 58-18 88-18s 58 18 88 18 58-18 88-18 58 18 88 18 v44h-352z" />
            </defs>
            <g class="parallax">
                <use xlink:href="#gentle-wave" x="48" y="5" fill="var(--bg-2)" />
                <use xlink:href="#gentle-wave" x="48" y="7" fill="var(--bg-1)" />
            </g>
        </svg>
    </div>
  </div>
  <div class="wcontent flex">
    <br>
    <br>
        <a href="/api/git"><i class="fa fa-github-square"></i></a>
  </div>
  <div class="footer">
    <div id="button"></div>
    <div id="container">
      <div id="cont">
      <div class="footer_center">
        <a href="/wzc/team">
